#include <iostream>
#include<climits>
using namespace std;

int main() 
{
  int n;
  cin>>n;
  int arr[n];
  for(int i=0;i<n;i++)
  {
    cin>>arr[i];
  }
  
  int max_element = arr[0];
  for(int i=0;i<n;i++)
  {
   
    cout<<max_element;
    if(arr[i+1]>max_element)
    {
      max_element=arr[i+1];
    }
    
  }
    

    
    
}
